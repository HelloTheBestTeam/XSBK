<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xsbk.article.mapper.ImageDetailMapper">
	<insert id="insertImageDetail" parameterType="com.xsbk.core.model.article.file.ImageDetail">
		insert into
		tb_image_detail(id,name,ext_name,size,create_time,owner_id,image_url,image_name)
		values(#{id},#{name},#{extName},#{size},#{createTime},#{ownerId},#{imageUrl},#{imageName})
	</insert>
</mapper>